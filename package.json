{
  "name": "ml-maid",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc && vite build && npm run rebuild:all && electron-builder",
    "build:x64": "vue-tsc && vite build && npm run rebuild:x64 && electron-builder --x64",
    "build:x86": "vue-tsc && vite build && npm run rebuild:x86 && electron-builder --ia32",
    "rebuild:all": "npm run rebuild:x64 && npm run rebuild:x86",
    "rebuild:x64": "npx electron-rebuild -f -w better-sqlite3 --arch=x64",
    "rebuild:x86": "npx electron-rebuild -f -w better-sqlite3 --arch=ia32",
    "preview": "vite preview"
  },
  "dependencies": {
    "better-sqlite3": "^12.2.0",
    "bindings": "^1.5.0",
    "echarts": "^5.6.0",
    "element-plus": "^2.10.4",
    "mitt": "^3.0.1",
    "pinia": "^3.0.3",
    "uuid": "^11.1.0",
    "vue": "^3.4.21",
    "vue-echarts": "^7.0.3",
    "vue-router": "^4.5.1"
  },
  "devDependencies": {
    "@tsconfig/node22": "^22.0.1",
    "@types/better-sqlite3": "^7.6.13",
    "@types/node": "^22.14.0",
    "@vitejs/plugin-vue": "^5.2.3",
    "@vue/tsconfig": "^0.7.0",
    "electron": "^30.0.1",
    "electron-builder": "^24.13.3",
    "electron-rebuild": "^3.2.9",
    "npm-run-all2": "^7.0.2",
    "typescript": "~5.8.0",
    "unplugin-auto-import": "^19.3.0",
    "unplugin-vue-components": "^28.8.0",
    "vite": "^6.2.4",
    "vite-plugin-electron": "^0.28.6",
    "vite-plugin-electron-renderer": "^0.14.5",
    "vite-plugin-vue-devtools": "^7.7.2",
    "vue-tsc": "^2.2.8"
  },
  "main": "dist-electron/main.js"
}
